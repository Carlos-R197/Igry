<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Igry.Views.HomeTabbedPage"
            xmlns:local="clr-namespace:Igry.Views"
            xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core"
            android:TabbedPage.ToolbarPlacement="Bottom"
            Title="Igry" 
            android:TabbedPage.IsSwipePagingEnabled="False" BarBackgroundColor="{StaticResource Key=DarkPrimaryColor}">
    <TabbedPage.Children>
        <local:HomePage/>
        <local:CatalogPage/>
        <local:SearchPage/>
        <local:SettingsPage/>
    </TabbedPage.Children>
</TabbedPage>